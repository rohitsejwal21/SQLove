/*
Norwegian Alpine Skiers
Find all Norwegian alpine skiers who participated in 1992 but didn't participate in 1994. Output unique athlete names.
*/

CODE:

SELECT
    DISTINCT
    name
FROM olympics_athletes_events
WHERE 
    team = 'Norway' AND
    sport = 'Alpine Skiing' AND
    year = 1992
EXCEPT
SELECT
    name
FROM olympics_athletes_events
WHERE 
    team = 'Norway' AND
    sport = 'Alpine Skiing' AND
    year = 1994
